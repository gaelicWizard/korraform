{
    "Statement": [
        {
            "Action": [
                "s3:GetObject",
                "s3:GetObjectVersion",
                "s3:GetBucketVersioning"
            ],
            "Resource": [
							"${S3_BUCKET}",
							"${S3_BUCKET}/*"
						],
            "Effect": "Allow"
        },
        {
            "Action": [
                "s3:PutObject",
                "s3:GetObject",
                "s3:DeleteObject"
            ],
            "Resource": [
							"${S3_BUCKET}",
							"${S3_BUCKET}/*"
						],
            "Effect": "Allow"
        },
				{
					"Effect": "Allow",
					"Action": [
						"codebuild:BatchGetBuilds",
						"codebuild:StartBuild"
					],
					"Resource": [
						"${CODEBUILD_ARN}"
					]
				}
    ],
    "Version": "2012-10-17"
}
