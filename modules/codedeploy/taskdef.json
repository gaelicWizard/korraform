    "containerDefinitions": [
        {
            "name": "${PROJECT_NAME}",
            "image": "<IMAGE1_NAME>",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "protocol": "tcp",
                    "containerPort": 80
                }
            ]
        },
        {
            "name": "${PROJECT_NAME}",
            "image": "<IMAGE2_NAME>",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "protocol": "tcp",
                    "containerPort": 80
                }
            ]
        },
        {
            "name": "${PROJECT_NAME}",
            "image": "<IMAGE3_NAME>",
